@Inbook{Kokke2015,
  author="Kokke, Pepijn
    and Swierstra, Wouter",
  editor="Hinze, Ralf
    and Voigtl{\"a}nder, Janis",
  title="Auto in Agda",
  bookTitle="Mathematics of Program Construction: 12th International Conference,
  MPC 2015, K{\"o}nigswinter, Germany, June 29--July 1, 2015. Proceedings",
  year="2015",
  publisher="Springer International Publishing",
  address="Cham",
  pages="276--301",
  abstract="As proofs in type theory become increasingly complex, there is a
    growing need to provide better proof automation. This paper shows how to
    implement a Prolog-style resolution procedure in the dependently typed
    programming language Agda. Connecting this resolution procedure to Agda's
    reflection mechanism provides a first-class proof search tactic for
    first-order Agda terms. As a result, writing proof automation tactics need
    not be different from writing any other program.",
  isbn="978-3-319-19797-5",
  doi="10.1007/978-3-319-19797-5_14",
  url="https://doi.org/10.1007/978-3-319-19797-5_14"
}


